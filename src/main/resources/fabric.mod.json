{
  "schemaVersion": 1,
  "id": "parabol-renderer",
  "version": "${version}",
  "name": "Parabol Renderer",
  "description": "Internal rendering library for Parabol.",
  "authors": ["Integr"],
  "contact": {
    "homepage": "-",
    "sources": "https://github.com/Integr-0/ParabolRenderer"
  },
  "license": "Apache-2.0",
  "icon": "assets/parabol-renderer/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.integr.parabol.renderer.ParabolRenderer"
    ]
  },
  "mixins": [
    "parabol-renderer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "modmenu": {
      "badges": [ "library" ],
      "parent___disabled": "parabol",
      "update_checker": false
    }
  }
}
